
<table class="table table-striped projects col-md-10 offset-md-1" border="4">
    <thead>
      <tr>
        <th>Fecha de Contratación</th>
        <th>Total en dólares de salarios pagados</th>
      </tr>
    </thead>
    <tbody>
      <% empleadosPorFecha.forEach((empleado) => { %>
        <tr>
          <td><%= new Date(empleado.fechaContratacion).toLocaleDateString('es-ES', {
            day: 'numeric',
            month: 'numeric',
            year: 'numeric',
          }) %></td>
          <td>$<%= empleado.totalSalarios %></td>
        </tr>
      <% }); %>
  
      <tr>
        <td>Total salarios pagados:</td>
        <td>$<%= totalSalariosGeneral %></td>
      </tr>
    </tbody>
  </table>
  